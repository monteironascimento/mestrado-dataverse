  },
  "homepage": "https://github.com/IQSS/dataverse-client-javascript#readme",
  "devDependencies": {
    "@types/chai": "^4.2.1",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.18",
    "@types/request-promise": "^4.1.44",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "sinon": "^7.4.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.2",
